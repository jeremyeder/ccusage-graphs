        "id": 1,
        "title": "Daily Cost Test",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "frser-sqlite-datasource",
              "uid": "${DS_CLAUDE_USAGE_SQLITE}"
