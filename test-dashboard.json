            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 10
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "currencyUSD"
          }
        },
        "options": {
          "legend": {
            "displayMode": "hidden"
          }
        }
      },
      {
        "id": 2,
        "title": "Total Records",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "frser-sqlite-datasource",
              "uid": "${DS_CLAUDE_USAGE_SQLITE}"
            },
            "rawSql": "SELECT COUNT(*) as value FROM daily_usage",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
